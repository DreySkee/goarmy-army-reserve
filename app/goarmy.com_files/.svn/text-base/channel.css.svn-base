    background: url(../images/left_nav/left_nav_arrow_right.png) no-repeat right 12px;
    cursor:pointer;
}
#left #left_nav ul li ul.one {padding:0;width:100%;}/*border-top:1px solid #E8E8E8;margin-top:10px;width:192px;}*/
