
body {background:#000;text-align:left;}
#channelWrapper {
    height:100%;
    width:100%;
    position:relative;
}
#channelMarquee {
    display:block;
    position: absolute;
    top:0;left:0;
    z-index: -1;
    height:100%;
    width:100%;
    background:top center black no-repeat;
}
#site_wrapper {z-index:5;}
.contentBrdr {
    padding-bottom: 0;
    position: relative;
    z-index: 0;
    border: none;
    width: auto;
}
#page_content {
    float:left;
    clear:both;
    -moz-border-radius: 5px 5px 0 0;
    border-radius: 5px 5px 0 0;
    border: none;
    height: auto;
    margin-top: 20px;
    position: relative;
    width: 960px;
}
.contentpar {
    float:left;
    position:relative;
}
#main_content {
    border-left: 1px solid #E8E8E8;
    float: left;
    padding: 15px 38px 50px 40px;
    min-height: 520px;
    text-align: left;
    width: 641px;
    margin:0;

}
div#sectionTitle {
    margin:11px 0 20px;
    position: relative;
    z-index: 2;
    height:auto;
    width:100%;
}
div#sectionTitle img  {
    padding:0;
}
.mainheader {
    position:relative;
    float:left;
    width:75%;
    margin-top:9px;
}
#main_content a:hover {color:#2B7CAD;text-decoration:underline;}
a, .darkLinks a, #jobsearch-wrapper .search-content a {color:#2B7CAD; text-decoration:none;}
#main_content a:visited:hover {text-decoration:underline;}
#main_content .breadcrumb {
    position:relative;
    float:left;
    width:100%;
}
#main_content .breadcrumb-wrapper {
    position:relative;
    float:left;
    width:100%;
    font-size:10px;
    text-transform:uppercase;
    font-weight:bold;
    top:6px;
    left:0;
}
.breadcrumb-wrapper a {
    text-decoration:none;
    padding-right:8px;
    color:#6a6967;
}
#main_content .breadcrumb-wrapper a:hover {color:#6a6967;}
#main_content .breadcrumb-wrapper a.current_page_bc,#main_content .breadcrumb-wrapper a.current_page_bc:hover {
    color:#9c9c9c;
}
.breadcrumb-wrapper .crumb {
    float:left;
    position:relative;
    height:16px;
    padding-bottom:4px;
}
.breadcrumb-wrapper .crumb span.breadcrumb-arrow {
    background:url(../images/breadcrumb/channel_breadcrumb_arrow.png) no-repeat 0 0;
    width:14px;
    height:16px;
    float:left;
    padding-right:8px;
}
.doubleline .single {
    border-bottom:1px solid;
}
.doubleline .double {
    border-bottom:1px solid;
    border-top:1px solid;
}
.doubleline .no_line {content
    border-bottom:none;
}

#headerImg img {border:1px solid #929292;}
.selectlinks { 
    position:absolute;
    top:69px; 
    right: 40px;
}
.selectlinks .select-links {
    color:#767676;
    font-size:12px;
    padding:3px 3px 5px;
}
#mainTranscripts {position:relative;float:left;}
#mainTranscripts ul.anchorLinks li a {color:#2B7CAD;}

/************* LEFT NAV ********************/
#left {
    float: left;
    display: block;
    height: 100%;
    width: 240px;
    margin:17px 0 30px;
    padding:0;
}
.long-left #left {
